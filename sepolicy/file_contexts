# Camera
/data/cam_socket([0-9])+                           u:object_r:camera_socket:s0

# Vibrator
/sys/class/timed_output/vibrator/amp               u:object_r:vibeamp_sysfs:s0

# Keydisabler
/sys/devices/gpio_keys.75/disabled_keys            u:object_r:display_sysfs:s0

# Double tap to wake
/sys/android_touch/doubletap2wake                  u:object_r:sysfs_hardware:s0

# GPS
/system/bin/gsiff_daemon                           u:object_r:location_exec:s0

# Sensors
/persist/misc/sensors(/.*)?                        u:object_r:sensors_persist_file:s0
